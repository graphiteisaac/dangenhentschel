pub fn main() {
  let app = lustre.application(init, update, view)
  let provided_hash = document_hash()
  let assert Ok(_) = lustre.start(app, "#app", provided_hash)

  Nil
}

fn init(hash: String) {
  #(
    colours
      |> list.find(fn(x) { x.hex == hash })
      |> result.unwrap(Colour(name: "red", hex: "#e50000")),
    effect.none(),
  )
}

type Msg {
  Random
  UserSetColour(hex: String)
  DocumentUpdatedHash
}

fn update(model, msg) {
  case msg {
    Random -> {
      let found =
        colours
        |> list.shuffle
        |> list.first
        |> result.unwrap(Colour(name: "red", hex: "#e50000"))
      #(found, update_hash_effect(found.hex))
    }
    UserSetColour(hex) -> #(
      colours
        |> list.find(fn(x) { x.hex == hex })
        |> result.unwrap(Colour(name: "red", hex: "#e50000")),
      update_hash_effect(hex),
    )
    _ -> #(model, effect.none())
  }
}

