@external(javascript, "./dangenhentschel_ffi.mjs", "document_hash")
fn document_hash() -> String

@external(javascript, "./dangenhentschel_ffi.mjs", "set_document_hash")
fn set_document_hash(hash: String) -> Nil

fn update_hash_effect(hash: String) {
  use dispatch, _ <- effect.after_paint
  set_document_hash(hash)
  dispatch(DocumentUpdatedHash)
}

